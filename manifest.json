{
    "manifest_version": 3,
    "name": "Price Tracker",
    "version": "1.0",
    "background": {
      "service_worker": "service-worker.js",
      "type":"module"
    },
    "content_scripts": [
      {
        "matches" : ["<all_urls>"],
        "js":["content.js"]
      }
    ]
    ,
    "action": {
      "default_popup": "popup.html",
      "default_icon":"icon.png"
    },
    "permissions": [
      "tabs",
      "activeTab",         
      "alarms",
      "storage"        
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }
  